{
  "version": "0.0.1",
  "apiVersion": "1.0.0",
  "author": "Tim Reyes",
  "description": "Analog measurement for min/max volts",
  "name": "Voltage Min and Max",
  "extensions": {
    "MinAndMax": {
      "type": "AnalogMeasurement",
      "entryPoint": "voltage_min_max.VoltageMinMaxMeasurer",
      "metrics": {
        "minimum": {
          "name": "Minimum",
          "notation": "V<sub>min</sub>",
          "units": "V"
        },
        "maximum": {
          "name": "Maximum",
          "notation": "V<sub>max</sub",
          "units": "V"
        }
      }
    }
  }
}
